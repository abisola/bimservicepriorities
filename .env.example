# Airtable Configuration
# Get your Personal Access Token from: https://airtable.com/create/tokens
# Note: API Keys are deprecated. Use Personal Access Tokens instead.
AIRTABLE_PERSONAL_ACCESS_TOKEN=your_personal_access_token_here

# Base ID can be found in the Airtable API documentation for your base
# Format: appXXXXXXXXXXXXXX
AIRTABLE_BASE_ID=your_base_id_here

# Table Names
# The table where services are stored (with columns: ServiceName, ServiceValue)
AIRTABLE_SERVICES_TABLE=Services

# The table where form submissions will be saved
AIRTABLE_SUBMISSIONS_TABLE=Submissions
